	<profiles>
		<profile>
			<id>opt</id>
			<activation>
				<activeByDefault>true</activeByDefault>
			</activation>
			<properties>
				<suffix>-opt</suffix>
			</properties>
			<dependencyManagement>
				<dependencies>
					<dependency>
						<groupId>com.weicoder.extend</groupId>
						<artifactId>extend</artifactId>
						<version>${extend}</version>
						<type>pom</type>
						<scope>import</scope>
					</dependency>
				</dependencies>
			</dependencyManagement>
		</profile>
		<profile>
			<id>native</id>
			<properties>
				<suffix></suffix>
			</properties>
		</profile>
		<profile>
			<id>jdk11</id>
			<properties>
				<suffix>-jdk11</suffix>
			</properties>
		</profile>
	</profiles>

