<project xmlns="http://maven.apache.org/POM/4.0.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
	<parent>
		<groupId>com.weicoder</groupId>
		<artifactId>weicoder</artifactId>
		<version>3.3.4</version>
	</parent>
	<modelVersion>4.0.0</modelVersion>
	<artifactId>xml</artifactId>


	<dependencies>
		<dependency>
			<groupId>com.weicoder</groupId>
			<artifactId>common</artifactId>
		</dependency> 
	</dependencies>

	<profiles>
		<profile>
			<id>opt</id>
			<activation>
				<activeByDefault>true</activeByDefault>
			</activation>
			<dependencies>
				<dependency>
					<groupId>com.weicoder.extend</groupId>
					<artifactId>xml-extend</artifactId>
				</dependency>
			</dependencies>
		</profile>
		<profile>
			<id>native</id>
			<dependencies>
				<dependency>
					<groupId>org.dom4j</groupId>
					<artifactId>dom4j</artifactId>
					<version>${dom4j}</version>
				</dependency>
				<dependency>
					<groupId>org.jdom</groupId>
					<artifactId>jdom2</artifactId>
					<version>${jdom2}</version>
				</dependency>
				<dependency>
					<groupId>com.thoughtworks.xstream</groupId>
					<artifactId>xstream</artifactId>
					<version>${xstream}</version>
				</dependency>
			</dependencies>
		</profile>
		<profile>
			<id>jdk11</id>
			<dependencies>
				<dependency>
					<groupId>com.weicoder.fork</groupId>
					<artifactId>dom4j-jdk11</artifactId>
					<version>${dom4j}</version>
				</dependency>
				<dependency>
					<groupId>com.weicoder.fork</groupId>
					<artifactId>jdom2-jdk11</artifactId>
					<version>${jdom2}</version>
				</dependency>
				<dependency>
					<groupId>com.weicoder.fork</groupId>
					<artifactId>xstream-jdk11</artifactId>
					<version>${xstream}</version>
				</dependency>
			</dependencies>
		</profile>
	</profiles>
</project>